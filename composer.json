{
    "name": "laraflat/users",
    "description": "laraflat package to manage user",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "Laraflat\\Users\\": "src/"
        },
        "classmap": [
            "database/seeds"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflat\\Users\\Providers\\LaraflatUserServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
          "php artisan make:auth",
          "php artisan migrate",
          "php artisan db:seed --class=AdminUser"
        ]
      }
}
