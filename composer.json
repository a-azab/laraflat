{
    "name": "laraflat/users",
    "description": "laraflat package to manage user",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Abdel Aziz hassan",
            "email": "zizo1999988@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "autoload": {
        "psr-4": {
            "Laraflat\\Users\\": "src/"
        },
        "classmap": [
            "src/database/seeds"
        ]
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflat\\Users\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflat\\Users\\Providers\\LaraflatUserServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
          "@php artisan make:auth",
          "@php artisan migrate",
          "@php artisan db:seed --class=AdminUser"
        ]
    }
}
