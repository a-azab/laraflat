{
    "name": "laraflat/users",
    "description": "laraflat package to manage user",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Abdel Aziz hassan",
            "email": "zizo1999988@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "~5.6.0"
    },
    "autoload": {
        "psr-4": {
            "Laraflat\\Users\\": "src/"
        },
        "classmap": [
            "src/database/seeds"
        ]
    },
    "require-dev": {
        "phpunit/phpcov": "~3.0|~4.0",
        "phpunit/phpunit": "~7.0",
        "laravel/framework": "~5.6.0",
        "orchestra/testbench": "~3.6.0",
        "orchestra/database": "~3.6.0",
        "orchestra/testbench-core": "~3.6.0",
        "laravel/browser-kit-testing": "~2.0.0",
        "orchestra/testbench-browser-kit": "~3.6.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Laraflat\\Users\\Tests": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraflat\\Users\\Providers\\LaraflatUserServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-install-cmd": [
          "@php artisan make:auth",
          "@php artisan migrate",
          "@php artisan db:seed --class=AdminUser"
        ]
    },
    "prefer-stable": true
}
